{
    "name": "dominiksenti/orange-swiss-payment-slip",
    "description": "Laravel Package for Creating Orange Swiss Payment Slips in PDF format using FPDF",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Senti",
            "email": "dominik@email.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Dominiksenti\\OrangeSwissPaymentSlip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dominiksenti\\OrangeSwissPaymentSlip\\Test\\": "tests/"
        }
    },
    "extra": {
	"laravel": {
	    "providers": [
		"Dominiksenti\\OrangeSwissPaymentSlip\\OrangeSwissPaymentSlipServiceProvider"
	    ],
	    "aliases": {
		"OrangeSwissPaymentSlip": "Dominiksenti\\OrangeSwissPaymentSlip\\OrangeSwissPaymentSlipFacade"
	    }
	}
    }
}
